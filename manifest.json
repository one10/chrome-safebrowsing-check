{
  "name": "Check a URL in blacklists",
  "description": "Checks a given URL in blacklists in background tabs",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Check a URL in blacklists",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
